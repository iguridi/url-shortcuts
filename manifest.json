{
    "name": "Url generator",
    "version": "0.0.1",
    "description": "Create url shortcuts with arguments",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "omnibox": {
        "keyword": "kk"
    },
    "permissions": [
        "declarativeContent",
        "storage"
    ],
    "browser_action": {
        "default_popup": "textarea.html",
        "default_icon": {
            "16": "images/16.png",
            "32": "images/32.png",
            "64": "images/64.png",
            "128": "images/128.png"
        }
    },
    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "64": "images/64.png",
        "128": "images/128.png"
    }
}

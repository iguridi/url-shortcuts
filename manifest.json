{
    "name": "Url generator",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "omnibox": {
        "keyword": "kk"
    },
    "permissions": [
        "declarativeContent",
        "storage"
    ],
    "browser_action": {
        "default_popup": "textarea.html"
    }
}
